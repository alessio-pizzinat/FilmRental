<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.filmrental.model.FilmRequest" table="FILMREQUEST">

		<id name="requestId" column="REQUEST_ID">
			<generator class="assigned" />
		</id>
		<property name="userId" column="USER_ID" insert="false" update="false"/>
		<property name="title" column="TITLE" />
		<property name="regist" column="REGIST" />
		<property name="exitYear" column="EXIT_YEAR" />
		
		<many-to-one name="user" class="com.filmrental.model.User"
			column ="USER_ID" />

	</class>
</hibernate-mapping>
 