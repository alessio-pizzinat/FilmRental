<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.filmrental.model.Film" table="FILMS">
		<id name="filmId" column="FILM_ID">
			<generator class="assigned" />
		</id>
		<property name="title" column="TITLE" />
		<property name="regist" column="REGIST" />
		<property name="exitYear" column="EXIT_YEAR" />
		<property name="quantity" column="QUANTITY" />

		<bag name="filmRented" table="FILMRENTED" inverse="true" lazy="false"
			fetch="select">
			<key column="USER_ID" />
			<one-to-many class="com.filmrental.model.FilmRent" />
		</bag>

	</class>
</hibernate-mapping>
 